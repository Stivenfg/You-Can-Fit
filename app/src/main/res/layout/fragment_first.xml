                            android:layout_width="40dp"
                            android:layout_height="34dp"
                            android:layout_marginEnd="20dp"
                            android:src="@drawable/boton_video"
                            app:layout_constraintBottom_toBottomOf="@id/itemEjercicio1"
                            app:layout_constraintEnd_toEndOf="@id/itemEjercicio1"
                            app:layout_constraintHorizontal_bias="1.0"
                            app:layout_constraintStart_toEndOf="@+id/btnSwitchFragment"
                            app:layout_constraintTop_toTopOf="@id/itemEjercicio1"
                            app:layout_constraintVertical_bias="0.384" />

